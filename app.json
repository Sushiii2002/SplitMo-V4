{
  "expo": {
    "name": "SplitMo",
    "slug": "SplitMo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CONTACTS"
      ]
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },

    "extra": {
      "eas": {
        "projectId": "e0988fd4-7f42-4c03-b844-28efc90248fe"
      }
    }
  }
}
